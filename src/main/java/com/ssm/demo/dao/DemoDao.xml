<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.ssm.demo.dao.DemoDao">

    <select id="getEmp" resultType="com.ssm.demo.entity.DemoEntity">
        select  e.ENAME ENAME,
                e.JOB JOB,
                e.SAL SAL
        from emp e
        where e.empno='7369'
    </select>

 </mapper>